{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/delete_contact",
      "methods": ["DELETE"],
      "dest": "/backend/main.py"
    },
    {
      "src": "/update_contact",
      "methods": ["PATCH"],
      "dest": "/backend/main.py"
    },
    {
      "src": "/create_contact",
      "methods": ["POST"],
      "dest": "/backend/main.py"
    },
    {
      "src": "/contacts",
      "methods": ["GET"],
      "dest": "/backend/main.py"
    }
  ]
}
